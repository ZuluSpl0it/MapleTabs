{
  "manifest_version": 2,

  "name": "MapleTabs",
  "description": "Intuitive browser navigation",
  "version": "1.0",

  "background": {
    "page": "background.html"
  },
 
 "icons": {
	"512": "mapleleaf_icon.png"
  },

  "browser_action": {
    "default_icon": "mapleleaf_icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ],
  "commands": {
    "shift_down": {
      "suggested_key": {
        "default": "Ctrl+Shift+M",
        "mac": "Command+Shift+M"
      },
      "description": "Tab Tree shift down key"
    },
    "shift_up": {
      "suggested_key": {
        "default": "Ctrl+Shift+K",
        "mac": "Command+Shift+K"
      },
      "description": "Tab Tree shift up key"
    }
  },
  "content_scripts": [
    {
      "matches": ["http://*/*",
                  "https://*/*"],
      "js": ["jquery.js", "notify.js"]
    }
  ]
}
